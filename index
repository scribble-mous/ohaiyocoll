                <script type="text/javascript">
                        var d = new Date();
                        var n = d.getHours();
 
                        if ( n == 23 || n == 0) {
                                message = "<img src='goodaftersleep1.png'></img>";
                        } else if ( n >= 1 && n <= 4 ) {
                                message = "<img src='goodafternoon1.png'></img>";
                        } else if ( n >= 5 && n <= 11 ) {
                                message = "<img src='goodafternoon1.png'></img>";
                        } else if ( n >= 12 && n <= 17 ) {
                                message = "<img src='goodafternoon1.png'></img>";
                        } else if ( n >= 18 && n <= 22 ) {
                                message = "<img src='goodafternoon1.png'></img>";
                        }
 
                        document.getElementById("message").innerHTML = message;
                </script>
